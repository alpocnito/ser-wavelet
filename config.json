{
    "input_path": "/media/greca/HD/Datasets/PROPOR 2022",
    "output_path": "./features",
    "sample_rate": 16000,
    "dataset": "propor2022",
    "wavelet": {
        "type": "dwt",
        "name": "db4",
        "level": 4,
        "mode": "symmetric"
    },
    "feature": {
        "name": "mfcc",
        "n_fft": 1024,
        "hop_length": 512,
        "n_mfcc": 128
    },
    "model": {
        "name": "cnn",
        "use_gpu": true,
        "output_path": "./checkpoints",
        "batch_size": 32,
        "learning_rate": 0.001,
        "epochs": 50
    },
    "kfold": {
        "num_k": 5
    },
    "overwrite": false,
    "to_mono": true
}